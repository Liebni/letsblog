{{ extend('layouts/admin/admin') }}

{{#block ('mainTitle')}}
	{{#if (info.status === 1)}}
	操作成功！
	{{elseif (info.status === 2)}}
	出错啦！
	{{else}}
	操作提示
	{{/if}}
{{/block}}

{{#block ('mainBody')}}
	<p>{{ info.message }}</p>
	{{#if (info.httpStatus === 500 && info.stack)}}<p>{{ info.stack }}</p>{{/if}}
	<p style="margin-top: 1em;"><a href="{{ info.backURL }}">&laquo; 返回上一页</a></p>
{{/block}}